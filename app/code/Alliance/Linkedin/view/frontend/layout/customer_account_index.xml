<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Account</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Alliance\Linkedin\Block\Linkedin\Edit" name="customer_account_dashboard_linkedin" as="linkedin" template="Alliance_Linkedin::linkedininformation.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
